        "**/tim_logs/**": true,
        "**/tim_rust/**": true,
        "**/screenshots/**": true,
        "**/Dumbo/**": true,
        "**/.git/**": true
    },
    "search.exclude": {
        "**/.mypy_cache/**": true,
        "**/.devcontainers/**": true,
        "**/.run/**": true,
        "**/.idea/**": true,
        "**/.venv/**": true,
        "**/node_modules/**": true,
        "**/__pycache__/**": true,
