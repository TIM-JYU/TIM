{
  "name": "TIM Container",
  "dockerComposeFile": ["../../docker-compose.yml"],
  "service": "tim",
  "workspaceFolder": "/service",
  "overrideCommand": true,
  "runServices": ["tim"],
  "shutdownAction": "none",
  "remoteUser": "root",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.mypy-type-checker",
        "ms-python.black-formatter",
        "Angular.ng-template",
        "esbenp.prettier-vscode",
        "dbaeumer.vscode-eslint",
        "wholroyd.jinja"
      ]
    }
  }
}
