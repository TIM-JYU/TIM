{% extends "base.jinja2" %}
{% block title %}{{ code }} {{ status }}{% endblock %}
{% block head %}
    {{ super() }}
    <script>
        var errorCode = {{ code }};
        var errorStatus = {{ status|tojson }};
    </script>
{% endblock %}
{% block content %}
    <p class="errormsg">{{ message }}</p>
{% endblock %}
