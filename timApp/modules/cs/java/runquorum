rm -rf Run
mkdir Run
ln -s /cs/java/quorum/Library Library 2>/dev/null
ln -s /cs/java/quorum/Quorum.jar Quorum.jar 2>/dev/null
ln -s /cs/java/quorum/QuorumStandardLibrary.jar QuorumStandardLibrary.jar 2>/dev/null 
ln -s /cs/java/quorum/QuorumStandardPlugins.jar QuorumStandardPlugins.jar 2>/dev/null
ln -s /cs/java/quorum/Run/Graphics Run/Graphics 2>/dev/null
ln -s /cs/java/quorum/Run/jni Run/jni 2>/dev/null 
ln -s /cs/java/quorum/Run/Robots Run/Robots 2>/dev/null 
java -jar /cs/java/quorum/Quorum.jar hello.q 
cp Run/Default.jar .
java -jar Default.jar

